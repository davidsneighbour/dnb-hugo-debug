  "devDependencies": {
    "@dnb-org/standard-version-config": "3.3.7",
    "@dnb-org/tools": "3.3.7"
  },
  "scripts": {
    "clean": "run-p clean:**",
    "clean:hugo": "rimraf node_modules resources package-lock.json public",
    "release": "npm run release:patch",
    "release:patch": "standard-version --release-as patch -a -t \"v\" && git push --follow-tags origin main",
    "release:major": "standard-version --release-as major -a -t \"v\" && git push --follow-tags origin main",
    "release:minor": "standard-version --release-as minor -a -t \"v\" && git push --follow-tags origin main"
  },
  "private": true
