{
  "name": "@dnb-hugo/debugprint",
  "description": "Hugo's missing debugging partial.",
  "version": "1.0.5",
  "author": {
    "name": "Kaushal Modi",
    "web": "https://github.com/kaushalmodi"
  },
  "bugs": {
    "url": "https://github.com/dnb-hugo/debugprint/issues"
  },
  "commitlint": {
    "extends": [
      "@dnb-hugo/commitlint-config"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@dnb-hugo/browserslist-config": "github:dnb-hugo/browserslist-config",
    "@dnb-hugo/commitlint-config": "github:dnb-hugo/commitlint-config",
    "@dnb-hugo/remark-preset-lint": "github:dnb-hugo/remark-preset-lint",
    "@dnb-hugo/standard-version-config": "github:dnb-hugo/standard-version-config",
    "@dnb-hugo/stylelint-config": "github:dnb-hugo/stylelint-config",
    "husky": "4.3.8",
    "npm-run-all": "4.1.5",
    "standard-version": "9.1.0",
    "stylelint": "13.9.0"
  },
  "homepage": "https://github.com/dnb-hugo/debugprint#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "GPL-3.0-only",
  "maintainer": {
    "name": "Patrick Kollitsch",
    "email": "patrick@davids-neighbour.com",
    "web": "https://davids-neighbour.com"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dnb-hugo/debugprint.git"
  },
  "scripts": {
    "clean": "run-p clean:**",
    "clean:npm": "rm -rf node_modules",
    "commitlint": "commitlint --from=HEAD~1",
    "lint": "run-p lint:**",
    "lint:scss": "stylelint assets/scss",
    "release": "standard-version --release-as patch -a -t \"v\" && git push --follow-tags origin main",
    "release-major": "standard-version --release-as major -a -t \"v\" && git push --follow-tags origin main",
    "release-next": "standard-version --release-as minor -a -t \"v\" && git push --follow-tags origin main"
  }
}
