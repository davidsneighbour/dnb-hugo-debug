{
  "name": "dnb-hugo-debug",
  "version": "1.4.4",
  "license": "GPL-3.0-only",
  "description": "GoHugo's missing debugging partials.",
  "author": {
    "name": "Kaushal Modi",
    "web": "https://github.com/kaushalmodi"
  },
  "maintainer": {
    "name": "Patrick Kollitsch",
    "email": "patrick@davids-neighbour.com",
    "web": "https://davids-neighbour.com"
  },
  "homepage": "https://github.com/dnb-org/dnb-hugo-debug#readme",
  "repository": "dnb-org/dnb-hugo-debug",
  "bugs": {
    "url": "https://github.com/dnb-org/dnb-hugo-debug/issues"
  },
  "dependencies": {
    "mem": "^9.0.2"
  },
  "devDependencies": {
    "@dnb-org/bootstrap-config": "^3.5.2",
    "@dnb-org/cypress-config": "^3.4.13",
    "@dnb-org/standard-version-config": "^3.4.10",
    "@dnb-org/tools": "^3.4.12"
  },
  "scripts": {
    "clean": "run-p clean:**",
    "clean:cypress": "rimraf tests/cypress/screenshots tests/cypress/videos",
    "clean:hugo": "rimraf dev/public dev/resources dev/.hugo_build.lock hugo.log",
    "clean:node": "rimraf node_modules package-lock.json",
    "release": "standard-version --release-as patch -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && ./bin/hooks/postrelease",
    "release:minor": "standard-version --release-as minor -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && ./bin/hooks/postrelease",
    "serve": "run-p serve:hugo",
    "serve:hugo": "bin/server/run",
    "server": "npm run serve",
    "test": "run-p test:server test:cypress",
    "test:cypress": "cypress run",
    "test:cypress:ide": "cypress open",
    "test:ide": "run-p test:server test:cypress:ide",
    "test:server": "./bin/dev/server"
  },
  "private": true
}
